{
  "name": "im-sergio",
  "version": "2.0.0",
  "description": "AI Innovation & Cloud Architecture Portfolio - Sergio Masellis",
  "scripts": {
    "dev": "parcel app/index.html --open",
    "start": "parcel app/index.html",
    "build": "parcel build app/index.html --dist-dir dist",
    "build:prod": "parcel build app/index.html --dist-dir dist --no-source-maps",
    "lint": "eslint 'app/js/**/*.{js,jsx,ts,tsx}' --fix",
    "format": "prettier --write 'app/**/*.{js,jsx,ts,tsx,json,css,scss,md}'",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "husky install"
  },
  "keywords": [
    "ai",
    "machine-learning",
    "cloud-architecture",
    "react",
    "typescript",
    "portfolio",
    "jpmorgan-chase"
  ],
  "author": "Sergio Masellis",
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-brands-svg-icons": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "three": "^0.167.1",
    "vanta": "^0.5.24"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.12.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "cssnano": "^7.0.4",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.2",
    "parcel": "^2.12.0",
    "prettier": "^3.2.5",
    "sass": "^1.77.2",
    "typescript": "^5.4.5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "> 0.3%",
    "not ie 11"
  ]
}
